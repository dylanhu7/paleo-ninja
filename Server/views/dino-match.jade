doctype html
html
  head
    title Your Dino Match
    link(rel="stylesheet",type="text/css",href="/css/dino-match.css")
    link(rel="stylesheet",type="text/css",href="/css/styles.css")
    link(rel="icon", href="favicon.ico")
  body
    .paleo-ninja
      span#paleo Paleo
      span#dash -
      span#ninja Ninja
    p.result= dino_result
    .form
      form.username-form(action='http://127.0.0.1:3000/dino-match-update' method='post')
        input.username-input(name='username' type='text' placeholder='Enter another Fortnite username...')
        input#go-button(type='submit' value='Find my Dino!')
